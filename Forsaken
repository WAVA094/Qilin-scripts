-- Aimbot –¥–ª—è 1x1x1x1
local function aimbot1x1x1x1(state)
    aimbot1x1 = state

    if state and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Name ~= "1x1x1x1" then
        Window:SendNotification({
            Title = "Wrong Character",
            Content = "Oops, your current character isn't 1x, this POSSIBLY can bug out!",
            Duration = 5
        })
        return 
    end

    if state then
        aimbot1x1loop = game.Players.LocalPlayer.Character.HumanoidRootPart.ChildAdded:Connect(function(child)
            if not aimbot1x1 then return end
            
            for _, v in pairs(aimbot1x1sounds) do
                if child.Name == v then
                    -- –õ–æ–≥–∏–∫–∞ aimbot...
                end
            end
        end)
    elseif aimbot1x1loop then
        aimbot1x1loop:Disconnect()
        aimbot1x1loop = nil
    end
end

-- –û—Å—Ç–∞–ª—å–Ω—ã–µ aimbot —Ñ—É–Ω–∫—Ü–∏–∏ –∞–Ω–∞–ª–æ–≥–∏—á–Ω–æ –∏—Å–ø—Ä–∞–≤–ª–µ–Ω—ã
