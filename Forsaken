-- Hitbox Expander Variables
local hitboxSize = 5
local hitboxTransparency = 0.5
local hitboxOn = false
local hitboxConnection = nil

-- Hitbox Expander Functions
local function updateHitboxes()
    for _, plr in game:GetService("Players"):GetPlayers() do
        if plr ~= game.Players.LocalPlayer and plr.Character and plr.Character:FindFirstChild("Head") then
            local head = plr.Character.Head
            head.Size = Vector3.new(hitboxSize, hitboxSize, hitboxSize)
            head.Transparency = hitboxTransparency
            head.CanCollide = false
            head.Massless = true
            local face = head:FindFirstChild("face")
            if face then face:Destroy() end
        end
    end
end

local function resetHitboxes()
    for _, plr in game:GetService("Players"):GetPlayers() do
        if plr ~= game.Players.LocalPlayer and plr.Character and plr.Character:FindFirstChild("Head") then
            local head = plr.Character.Head
            head.Size = Vector3.new(1, 1, 1) -- Default head size
            head.Transparency = 0
            head.CanCollide = true
            head.Massless = false
        end
    end
end

-- Add Hitbox Expander Toggle
Tab5:AddToggle({
    Name = "HITBOX EXPANDER",
    Default = false,
    Callback = function(Value)
        hitboxOn = Value
        if hitboxOn then
            -- Connect to Heartbeat to update hitboxes continuously
            hitboxConnection = game:GetService("RunService").Heartbeat:Connect(function()
                updateHitboxes()
            end)
        else
            -- Disconnect and reset hitboxes
            if hitboxConnection then
                hitboxConnection:Disconnect()
                hitboxConnection = nil
            end
            resetHitboxes()
        end
    end
})

-- Add Hitbox Size Slider
Tab5:AddSlider({
    Name = "HITBOX SIZE",
    Min = 1,
    Max = 100,
    Increase = 1,
    Default = 5,
    Callback = function(Value)
        hitboxSize = Value
        -- Update hitboxes immediately when slider changes
        if hitboxOn then
            updateHitboxes()
        end
    end
})

-- Add Hitbox Transparency Slider
Tab5:AddSlider({
    Name = "HITBOX TRANSPARENCY",
    Min = 0,
    Max = 1,
    Increase = 0.1,
    Default = 0.5,
    Callback = function(Value)
        hitboxTransparency = Value
        -- Update hitboxes immediately when slider changes
        if hitboxOn then
            updateHitboxes()
        end
    end
})
