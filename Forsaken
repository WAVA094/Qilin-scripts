                    end
                end
                task.wait(0.1)
            end 
        end)
    end
})

Tab2:AddToggle({
    Name = "AUTO KILL SURVIVOR",
    Default = false,
    Callback = function(Value)
        ActiveAutoKillSurvivors = Value 
        task.spawn(function()
